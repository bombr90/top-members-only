<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head'); %>
  </head>
  <body class="container">
    <header>
      <%- include('./partials/header'); -%>
    </header>
    <main>
      <h1><%= title %></h1>
        <div class="cardStack">
          <div class="card">
            <ul>
            <%for (const [key,value] of Object.entries(member)) {%>
              <li><%=`${key}: ${value}`%></li>
            <%}%>
            </ul>
          </div>
        </div>
      <% if (typeof(errors) !== 'undefined') { %>
        Error List:
        <ul>
          <% errors.forEach(error => { %>
            <li><%= error.msg %></li>
          <% }) %>
        </ul>
      <% } %>
    </main>
    <footer>
      <%- include('./partials/footer'); -%>
    </footer>
  </body>
</html>